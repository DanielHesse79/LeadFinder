# Testing Environment Configuration
FLASK_ENV=testing
FLASK_DEBUG=True
LOG_LEVEL=DEBUG

# API Keys (use test keys or empty for testing)
SERPAPI_KEY=test_key
SWECRIS_API_KEY=test_key
CORDIS_API_KEY=test_key
NIH_API_KEY=test_key

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=mistral
OLLAMA_TIMEOUT=10

# Flask Configuration
FLASK_SECRET_KEY=test-secret-key
FLASK_HOST=127.0.0.1
FLASK_PORT=5051

# Research Configuration
RESEARCH_MAX_RESULTS=10
RESEARCH_TIMEOUT=10
DEFAULT_RESEARCH_QUESTION=epigenetics and pre-diabetes

# External APIs
ORCID_BASE_URL=https://pub.orcid.org/v3.0
PUBMED_BASE_URL=https://eutils.ncbi.nlm.nih.gov/entrez/eutils

# Logging
LOG_FILE=test.log

# Performance
REQUEST_POOL_SIZE=5
REQUEST_TIMEOUT=5
MAX_TEXT_LENGTH=500

# Export settings
EXPORT_FOLDER=test_exports
SCIHUB_FOLDER=test_scihub 
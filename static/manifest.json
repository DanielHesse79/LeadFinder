      "purpose": "any"
    },
    {
      "src": "/static/images/4Front2market_logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "/static/images/screenshot-desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "4Front2Market Dashboard - Desktop View"
    },
    {
      "src": "/static/images/screenshot-mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "4Front2Market Dashboard - Mobile View"
    }
  ],
  "shortcuts": [
    {
      "name": "Search Leads",
      "short_name": "Search",
      "description": "Search for new leads",
      "url": "/search",
      "icons": [
        {
          "src": "/static/images/4Front2market_logo.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "View Dashboard",
      "short_name": "Dashboard",
      "description": "View application dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/static/images/4Front2market_logo.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Configuration",
      "short_name": "Config",
      "description": "Manage application settings",
      "url": "/config",
      "icons": [
        {
          "src": "/static/images/4Front2market_logo.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+4front2market",
      "url": "/?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "application/vnd.ms-excel": [".xls"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "permissions": [
    "notifications",
    "background-sync"
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
  "developer": {
    "name": "4Front2Market Team",
    "url": "https://4front2market.com"
  },
  "version": "1.0.0",
  "minimum_version": "1.0.0"
} 